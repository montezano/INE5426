classe minha_classe{
	
	publico funcao alguma_coisa (inteiro valor1, fracionario valor2){
	
		texto valor3 = "um texto qualQUER";
		constante logico const1 = verdadeiro; 

		se (const1 == falso){

		} senao { 
			se(const1 == verdadeiro){

			}senao{

			}

		}

		escolha (valor3){

			caso "valor"  : retorno valor1;
			caso "valor2" : valor3 = "valor1"; 
			caso "valor3" : parar;
		}

	} 	
}